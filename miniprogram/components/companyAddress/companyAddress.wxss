/** 骨架屏 start */
.skeleton {
  opacity: 1;
  animation-fill-mode: both;
  background-color: #f0f2f5;
  animation: bounce 1s ease-in 0s infinite normal both;
}

@keyframes bounce {
  0% {
    opacity: 1;
  }
  50% {
    opacity: 0.3;
  }
  100% {
    opacity: 1;
  }
}

/** 骨架屏 end */
.address {
  width: 100%;
  margin-bottom: 42rpx;
}

.address-con {
  width: 450rpx;
  height: 65rpx;
  padding-left: 24rpx;
  padding-right: 24rpx;
  margin-left: 34rpx;
  font-size: 27rpx;
  font-family: Helvetica;
  color: #212b36;
  background: white;
  box-shadow: 0rpx 2rpx 0rpx 0rpx rgba(22, 29, 37, 0.05);
  border-radius: 5rpx;
  border: 2rpx solid #d6d6d6;
}

.address-con.disabld {
  border: none;
  box-shadow: none;
}

.icon {
  font-size: 40rpx;
}

.jiantou {
  font-size: 22rpx;
}

.address-name {
  font-size: 28rpx;
  font-weight: 500;
  line-height: 40rpx;
  color: #454e56;
  font-family: PingFangSC-Medium, PingFang SC;
}

.address-desc {
  font-size: 20rpx;
  font-weight: 500;
  line-height: 25rpx;
  color: #454e56;
  font-family: PingFangSC-Medium, PingFang SC;
}

.skeleton-address-name {
  height: 40rpx;
