<!--components/datePicker/datePicker.wxml-->
<view class="time flex-center">
  <view class="iconfont shijian"></view>
  <view class="time-con rowlc">
    <picker mode="date" value="{{date}}" start="09:01" end="21:01" bindchange="bindTimeChange">
      <view class="picker">
        当前选择: {{date}}
      </view>
    </picker>
  </view>
</view>
